{
  "name": "@koyalite/sdk",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
