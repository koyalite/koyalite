    "axios": "^1.0.0",
    "axios-retry": "^4.0.0",
    "eventsource": "^2.0.0",
    "ws": "^8.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@koyalite/tsconfig": "workspace:*",
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "msw": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
} 